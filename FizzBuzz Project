Integer num = 0;

do
{
    if(Math.mod(num,3) == 0)
    {
        if(Math.mod(num,5) == 0)
        {
            System.debug('fizzbuzz');
        } else 
        {

            System.debug('fizz');
        }
    }
    
    else if(Math.mod(num,5) == 0)
    {
        System.debug('buzz');
    } else 
    {
        System.debug(num);
    }
    num++;
} while (num <= 100);
